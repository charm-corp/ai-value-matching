{
  "version": 2,
  "name": "ai-matching-charm-rls",
  "builds": [
    {
      "src": "api/serverless-rls.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/serverless.js",
      "use": "@vercel/node"
    },
    {
      "src": "senior-ui.html",
      "use": "@vercel/static"
    },
    {
      "src": "senior-ui.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/rls/(.*)",
      "dest": "/api/serverless-rls.js"
    },
    {
      "src": "/api/health/enhanced",
      "dest": "/api/serverless-rls.js"
    },
    {
      "src": "/api/users/rls",
      "dest": "/api/serverless-rls.js"
    },
    {
      "src": "/api/matching/enhanced-test",
      "dest": "/api/serverless-rls.js"
    },
    {
      "src": "/api/cache/test",
      "dest": "/api/serverless-rls.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/serverless-rls.js"
    },
    {
      "src": "/senior-ui.html",
      "dest": "/senior-ui.html"
    },
    {
      "src": "/senior-ui.js",
      "dest": "/senior-ui.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/serverless-rls.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "FEATURE_AI_INSIGHTS": "true",
    "FEATURE_REAL_TIME_CHAT": "false",
    "FEATURE_ENHANCED_AUTH": "false",
    "CACHE_TTL": "300",
    "MEMORY_THRESHOLD": "256",
    "RESPONSE_TIME_THRESHOLD": "5000"
  }
}
