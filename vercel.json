{
  "version": 2,
  "name": "ai-matching-charm",
  "builds": [
    {
      "src": "api/serverless.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/serverless.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/serverless.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
