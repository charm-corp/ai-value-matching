{
  "version": 2,
  "name": "ai-matching-charm",
  "builds": [
    {
      "src": "api/serverless.js",
      "use": "@vercel/node"
    },
    {
      "src": "senior-ui.html",
      "use": "@vercel/static"
    },
    {
      "src": "senior-ui.js", 
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/serverless.js"
    },
    {
      "src": "/senior-ui.html",
      "dest": "/senior-ui.html"
    },
    {
      "src": "/senior-ui.js",
      "dest": "/senior-ui.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/serverless.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
